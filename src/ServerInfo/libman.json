{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "provider": "jsdelivr",
      "library": "@tabler/core@1.0.0-beta20",
      "destination": "wwwroot/lib/tabler-core/",
      "files": [ "!**", "dist/**", "!dist/libs/**" ]
    },
    {
      "provider": "jsdelivr",
      "library": "@tabler/icons@1.70.0",
      "destination": "wwwroot/lib/tabler-icons/",
